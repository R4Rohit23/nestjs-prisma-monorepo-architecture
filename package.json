{
  "name": "nestjs-prisma-monorepo-architecture",
  "version": "0.0.1",
  "description": "Monorepo for multi-lambda functions with CDK infrastructure",
  "author": "Rohit Chaware",
  "type": "module",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "pnpm build:shared && pnpm build:infra && pnpm build:lambdas",
    "build:layers": "pnpm build:layer:nest && pnpm build:layer:auth && pnpm build:layer:aws",
    "build:layer:nest": "cd layers/nest/nodejs && npm install && npm prune --production",
    "build:layer:auth": "cd layers/auth/nodejs && npm install && npm prune --production",
    "build:layer:aws": "cd layers/aws/nodejs && npm install && npm prune --production",
    "build:shared": "pnpm -r --filter \"./shared/*\" run build",
    "build:lambdas": "pnpm -r --filter \"./lambdas/*\" run build",
    "build:infra": "pnpm --filter @project-name/infrastructure run build",
    "deploy": "pnpm --filter @project-name/infrastructure run deploy",
    "deploy:dev": "pnpm --filter @project-name/infrastructure run deploy:dev",
    "deploy:prod": "pnpm --filter @project-name/infrastructure run deploy:prod",
    "synth": "pnpm --filter @project-name/infrastructure run synth",
    "diff": "pnpm --filter @project-name/infrastructure run diff",
    "lint": "eslint \"lambdas/**/*.ts\" \"infrastructure/**/*.ts\" \"shared/**/*.ts\"",
    "format": "prettier --write \"lambdas/**/*.ts\" \"infrastructure/**/*.ts\" \"shared/**/*.ts\"",
    "test": "pnpm --filter @project-name/lambda-function-x run test",
    "clean": "pnpm -r exec rm -rf dist node_modules/.cache 2>/dev/null || true",
    "generate:prisma": "cd layers/prisma/nodejs && npm run generate:prisma && cd ../../..",
    "generate:prisma:safe": "pnpm install --no-frozen-lockfile && npx prisma generate --schema ./layers/prisma/nodejs/prisma/schema/schema.prisma",
    "migrate:prisma": "npx prisma migrate dev --schema ./layers/prisma/nodejs/prisma/schema/schema.prisma"
  },
  "pnpm": {
    "overrides": {
      "@aws-sdk/client-lambda": "^3.0.0",
      "confbox": "^0.2.3"
    },
    "packageExtensions": {
      "prisma": {
        "peerDependencies": {
          "confbox": "^0.2.3"
        }
      }
    }
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.983.0",
    "@aws-sdk/client-sqs": "^3.974.0",
    "@aws-sdk/s3-request-presigner": "^3.983.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/adapter-pg": "7.2.0",
    "@prisma/client": "7.2.0",
    "@vendia/serverless-express": "^4.12.6",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "nodemailer": "^7.0.12",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.17.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/aws-lambda": "^8.10.159",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.6",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.24.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
